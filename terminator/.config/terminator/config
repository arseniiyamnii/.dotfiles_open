[global_config]
[keybindings]
  close_term = <Alt>q
  split_horiz = <Alt>minus
  split_vert = <Alt>backslash
  toggle_zoom = <Alt>f
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
[profiles]
  [[default]]
    cursor_color = "#aaaaaa"
    show_titlebar = False
